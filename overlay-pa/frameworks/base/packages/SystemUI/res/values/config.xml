<resources>
    <!-- Color of the FOD view -->
    <color name="config_udfpsColor">#00ffffff</color>
    
    <integer name="config_udfpsHbmType">0</integer>
    <integer name="config_udfpsVendorCode">0</integer>

    <!-- If not zero, limits the internal screen recorder's framerate to the set value. -->
    <integer name="config_screenRecorderMaxFramerate">60</integer>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">6</integer>
    <integer name="config_maxVisibleNotificationIconsOnLock">7</integer>

    <!-- Whether usage of the proximity sensor during doze is supported -->
    <bool name="doze_proximity_sensor_supported">false</bool>

    <!-- Whether to enable Lineage Health Service -->
    <bool name="config_lineageHealthSupported">true</bool>
</resources>
