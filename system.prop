# Bluetooth
vendor.bluetooth.soc=cherokee

# CNE
persist.vendor.cne.feature=1

# Data
persist.data.df.dev_name=rmnet_usb0
persist.vendor.data.profile_update=true

# Display
debug.sf.enable_hwc_vds=0

# DPM
persist.vendor.dpm.feature=1
persist.vendor.dpm.loglevel=0
persist.vendor.dpm.nsrm.bkg.evt=3955

# Deep Sleep
pm.sleep_mode=1
vendor.post_boot.parsed=1

# Dex
dalvik.vm.dex2oat64.enabled=true
persist.bg.dexopt.enable=true
dalvik.vm.dexopt.secondary=true

# Graphics
ro.surface_flinger.max_virtual_display_dimension=4096
ro.surface_flinger.vsync_event_phase_offset_ns=2000000
ro.surface_flinger.vsync_sf_event_phase_offset_ns=6000000
persist.demo.hdmirotationlock=false
persist.hwc.enable_vds=1

# Media
media.settings.xml=/vendor/etc/media_profiles_vendor.xml
audio.offload.video=true
media.stagefright.enable-player=true
media.stagefright.enable-http=true
media.stagefright.enable-aac=true
media.stagefright.enable-qcp=true
media.stagefright.enable-fma2dp=true
media.stagefright.enable-scan=true
media.stagefright.thumbnail.prefer_hw_codecs=true
mmp.enable.3g2=true
media.aac_51_output_enabled=true
persist.mm.enable.prefetch=true
ro.media.recorder-max-base-layer-fps=60

# Memory optimizations
ro.vendor.qti.sys.fw.bservice_enable=true
ro.min_pointer_dur=1
ro.securestorage.knox=false
windowsmgr.max_events_per_sec=150
ro.vendor.perf.scroll_opt=true
touch.pressure.scale=0.1
debug.kill_allocating_task=0
force_hw_ui=true
ro.product.gpu.driver=1
ENFORCE_PROCESS_LIMIT=false
ro.wmt.blcr.enable=0
persist.sys.use_dithering=0
persist.sys.use_16bpp_alpha=1
debug.performance.tuning=1
persist.sys.shutdown.mode=hibernate
ro.config.hw_quickpoweron=true
ro.HOME_APP_ADJ = 1
ro.config.sdha_apps_bg_max=64
ro.config.sdha_apps_bg_min=8
persist.purgeable_assets=1
persist.service.pcsync.enable=0
persist.service.lgospd.enable=0
net.dns1=1.1.1.1
net.dns2=1.0.0.1

#Netflix custom property
ro.netflix.bsp_rev=Q6150-17263-1

# system prop for NFC DT
ro.nfc.port=I2C
persist.nfc.smartcard.recorder.enable=true

# Radio
DEVICE_PROVISIONED=1
persist.sys.radio.force_lte_ca=true
persist.sys.fflag.override.settings_network_and_internet_v2=true
persist.vendor.ims.disableADBLogs=1
persist.vendor.ims.disableIMSLogs=1
persist.vendor.radio.enable_temp_dds=true
persist.vendor.radio.force_on_dc=true
ril.subscription.types=RUIM
use.voice.path.for.pcm.voip=true
ro.vendor.use_data_netmgrd=true
persist.data.wda.enable=true
persist.data.df.dl_mode=5
persist.data.df.ul_mode=5
persist.data.df.agg.dl_pkt=10
persist.data.df.agg.dl_size=4096
persist.data.df.mux_count=8
persist.data.df.iwlan_mux=9
persist.vendor.radio.atfwd.start=true

# Radio Powersaving
persist.radio.add_power_save=1
pm.sleep_mode=1
ro.ril.disable.power.collapse=0
ro.ril.fast.dormancy.rule=1
ro.ril.fast.dormancy.timeout=3
ro.mot.eri.losalert.delay=100
ro.vendor.use_data_netmgrd=true
ro.vold.umsdirtyratio=20
power.saving.mode=1

# SkiaGL Threaded
debug.renderengine.backend=skiaglthreaded
debug.hwui.renderer=skiagl
renderthread.skia.reduceopstasksplitting=true

# Time
persist.vendor.delta_time.enable=true

# WiFi
config.disable_rtt=true

# Wireless display
persist.debug.wfd.enable=1
persist.sys.wfd.virtual=0

# ZRAM writeback
ro.zram.mark_idle_delay_mins=60
ro.zram.first_wb_delay_mins=180
ro.zram.periodic_wb_delay_hours=24

# Face unlock
ro.face.sense_service.camera_id=1
